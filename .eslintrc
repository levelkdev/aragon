{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "standard",
    "standard-react",
    "plugin:prettier/recommended",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "plugins": ["prettier", "react"],
  "rules": {
    "valid-jsdoc": "error",
    "react/prop-types": 'warn',
    "linebreak-style": ["error", "unix"]
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.6"
    },
  }
}
